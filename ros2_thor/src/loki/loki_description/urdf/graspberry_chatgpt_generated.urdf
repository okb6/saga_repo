  <!-- MATERIAL DEFINITIONS -->
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="red">
    <color rgba="1 0 0 1"/>
  </material>
  <material name="green">
    <color rgba="0 1 0 1"/>
  </material>
  <material name="alu">
    <color rgba="0.75 0.75 0.75 1"/>
  </material>
  <!-- CREATE ROBOT -->
  <!-- Create base link -->
  <link name="/base_link"/>
  <link name="/top0">
    <inertial>
      <mass value="10.21"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.187252250833" ixy="0.0" ixz="0.0" iyy="0.209836770833" iyz="0.0" izz="0.119499541667"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="package://loki_description/meshes/tower_modules/DUMMY-TOWER-M.stl"/>
        <box size="0.265 0.209 0.42"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://loki_description/meshes/tower_modules/DUMMY-TOWER-M.stl"/>
        <box size="0.265 0.209 0.42"/>
      </geometry>
    </collision>
  </link>
  <joint name="/base_linktop0" type="fixed">
    <parent link="/base_link"/>
    <child link="/top0"/>
    <origin rpy="0 0 4.71238898038" xyz="-0.782 -0.658 0.65"/>
    <axis xyz="0 0 1"/>
  </joint>
  <link name="/leg0">
    <inertial>
      <mass value="15.64"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.035581" ixy="0.0" ixz="0.0" iyy="0.035581" iyz="0.0" izz="0.012512"/>
    </inertial>
    <visual>
      <origin rpy="0 0 4.71238898038" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://loki_description/meshes/drive_modules/836745-5-DUMMY-RevA.stl"/>
        <cylinder length="-0.15" radius="0.04"/>
        <!--mesh filename="package://thorvald_base/meshes/top_chassis.dae"/-->
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin rpy="0 0 4.71238898038" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://loki_description/meshes/drive_modules/836745-5-DUMMY-RevA.stl"/>
        <cylinder length="-0.15" radius="0.04"/>
      </geometry>
    </collision>
  </link>
  <joint name="/steering0" type="continuous">
    <parent link="/base_link"/>
    <child link="/leg0"/>
    <origin rpy="0 0 1.57079632679" xyz="-0.782 -0.658 0.2"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="100" friction="10000"/>
  </joint>
  <transmission name="steering0_tran">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="/steering0">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="steering0_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  <link name="/wheel0_link">
    <inertial>
      <mass value="6.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0728" ixy="0.0" ixz="0.0" iyy="0.0728" iyz="0.0" izz="0.12"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.16" radius="0.2"/>
        <!-- <box size = "0.00001 0.00001 0.00001"/> -->
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.16" radius="0.2"/>
        <!-- <box size = "0.00001 0.00001 0.00001"/> -->
      </geometry>
    </collision>
  </link>
  <gazebo reference="/wheel0_link">
    <material>Gazebo/Black</material>
    <mu1 value="2"/>
    <mu2 value="2"/>
  </gazebo>
  <joint name="/wheel0" type="continuous">
    <parent link="/leg0"/>
    <child link="/wheel0_link"/>
    <origin rpy="-1.57079632679 0 0" xyz="0 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="100" friction="1000"/>
  </joint>
  <transmission name="wheel0_tran">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="/wheel0">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="wheel0_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
